<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% users.each do |user| %>
     <tr>
      <%#= attachment_image_tag user, :profile_image, :fill, 100, 100, fallback: "icon.jpeg", size: "100Ã—100" %>
      <td><%= attachment_image_tag user, :profile_image, size:"60x60", fallback: "icon.jpg" %></td>
      <td><%= user.name %></td>
      <td><%= link_to 'Show', user, class: "user_#{user.id}" %></td>
     </tr>
    <% end %>
  </tbody>
</table>