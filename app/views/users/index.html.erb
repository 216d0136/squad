<table class='table'>
    <tr>
      <th>image</th>
      <th>team_name</th>
      <th colspan="3"></th>
    </tr>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%#= attachment_image_tag user, :profile_image_id, :fill, 50, 50 %></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user, class: "user_#{user.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>




